{
    "presets": [
        [
            "@babel/preset-env"
        ],
        [
            "@babel/stage-0",
            {
                "decoratorsLegacy": true
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        [
            "import",
            {
                "libraryName": "antd"
            }
        ]
    ]
}